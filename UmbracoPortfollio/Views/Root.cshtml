@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using ClientDependency.Core.Mvc
@{
    Layout = "Master.cshtml";
    Html.RequiresCss("~/css/font-awesome.min.css");
    Html.RequiresJs("http://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js", 0);
    //Html.RequiresJs("~/scripts/modernizr-2.6.2.js", 0);
    Html.RequiresJs("~/scripts/owl.carousel.min.js", 2);
    Html.RequiresCss("~/css/owl.carousel.min.css", 2);
    Html.RequiresCss("~/Content/style.css");
}
@section GoogleAnalytics
{
    @Umbraco.Field("analyticsCode", recursive: true)
}
@section Scripts
{
    @RenderSection("Scripts", required: false)

}
@RenderBody()