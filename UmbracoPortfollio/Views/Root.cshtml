@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using ClientDependency.Core.Mvc
@{
    Layout = "Master.cshtml";
    Html.RequiresJs("https://code.jquery.com/jquery-2.1.4.min.js", 0);
    Html.RequiresJsFolder("~/Scripts/Build");
    Html.RequiresCssFolder("~/Content/Build");
}
@section GoogleAnalytics
{
    @Umbraco.Field("analyticsCode", recursive: true)
}
@section Scripts
{
    @RenderSection("Scripts", required: false)
    <script>
      $(document).foundation();
    </script>
}
@RenderBody()