@inherits Umbraco.Web.Mvc.UmbracoViewPage<MasterViewModel>
@using ClientDependency.Core.Mvc
@using UmbracoPortfollio.Logic.Models;
@{
	Layout = null;
}
<!DOCTYPE html>
<html lang="en" prefix="og: http://ogp.me/ns#" itemscope="" itemtype="http://schema.org/WebPage" >
    <head>
        <meta charset="utf-8">
        <!--[if IE]> <meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'> <![endif]-->
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta http-equiv='Content-Type' content='text/html; charset=utf-8'>
        <link rel="dns-prefetch" href="https://code.jquery.com/">
        @Html.Partial("Icons")
        @Html.Partial("SEO")
        @RenderSection("Metadata", required: false)   
        @Html.RenderCssHere()
	    <!-- Google Analytics -->
	    @RenderSection("GoogleAnalytics", required: false)
    </head>
    <body id="page-top">           
        <div class="document off-canvas-wrapper" role="document">
            <div class="off-canvas-wrapper-inner" data-off-canvas-wrapper>
                @Html.Partial("Naviagtion")
                <div class="main off-canvas-content" data-off-canvas-content itemprop="mainContentOfPage">
                    @RenderBody()
                    <div class="js-off-canvas-exit"></div>
                </div>
            </div>
            <!-- Copyright Footer -->
            @Html.Partial("Footer")
        </div>
        <!--// Copyright Footer-->
        @Html.RenderJsHere()
        @Html.Partial("Scripts")
        @RenderSection("Scripts", required: false)
    </body>		
</html>
